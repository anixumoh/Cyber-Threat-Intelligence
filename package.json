{
  "name": "blueliv-html",
  "version": "0.2.10",
  "retailVersion": "4.0.BETA",
  "license": "MIT",
  "private": true,
  "scripts": {
    "install:local": "npm i",
    "install:ci": "npm i",
    "watch": "concurrently \"npm run watch:angular\" \"npm run watch:svelte\"",
    "watch:angular": "npm --prefix angular/ run watch:angular",
    "watch:svelte": "vite dev",
    "build:svelte": "vite build",
    "build:angular": "npm --prefix angular/ run build:angular",
    "build:global-search-peggy": "peggy src/lib/tcx-global-search/peggy.pegjs -o src/lib/tcx-global-search/peggy.ts --format es",
    "build": "sh build.sh",
    "ts-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "ts-check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "start": "npm run watch",
    "lint-diff": "git diff --diff-filter=d --name-only HEAD | grep -E '(\\.ts?|\\.html)$' | xargs eslint -c .eslintrc.json",
    "lint": "eslint --ext ts -c .eslintrc.json ./src",
    "lint-security": "eslint --ext ts -c .eslintrc-security.json ./src",
    "lint:fix": "eslint --ext ts -c .eslintrc.json ./src --fix",
    "format": "git diff --diff-filter=d --name-only HEAD | grep -E '(\\.ts?|\\.html|\\.scss|\\.json)$' | xargs prettier --write ",
    "precheck": "yarn format && yarn lint",
    "commit": "yarn precheck && git add . && echo '\u001b[33mRemember to commit!!!!\u001b[0m' && echo '\u001b[32mgit commit -m \"feat|bug(TC-XXXX) message\"\u001b[0m'",
    "postinstall": "npm --prefix angular/ i -f",
    "security:audit": "npm audit --level=moderate",
    "dependencies:outdated": "npm outdated",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.38",
    "@amcharts/amcharts4-geodata": "^4.1.27",
    "@felte/validator-yup": "^1.1.3",
    "clipboard-copy": "^4.0.1",
    "cookie": "^0.5.0",
    "core-js": "~3.32.2",
    "css-star-rating": "^1.3.1",
    "dayjs": "^1.11.9",
    "felte": "^1.2.12",
    "file-saver": "~2.0.5",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "mark.js": "^8.11.1",
    "marked": "^8.0.1",
    "uuid": "^9.0.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.24.1",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/svelte": "^4.0.3",
    "@types/cookie": "^0.5.2",
    "@types/d3": "^7.4.0",
    "@types/lodash": "^4.14.198",
    "@types/mark.js": "^8.11.8",
    "@types/marked": "^5.0.1",
    "@types/node": "^20.5.9",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitest/coverage-c8": "^0.30.1",
    "autoprefixer": "^10.4.15",
    "axios": "^1.5.0",
    "carbon-components-svelte": "^0.80.0",
    "carbon-icons-svelte": "^12.3.0",
    "carbon-preprocess-svelte": "^0.10.0",
    "concurrently": "^8.2.1",
    "cypress": "^10.0.0",
    "eslint": "^8.48.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jsdoc": "^46.5.1",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-no-wildcard-postmessage": "^0.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prototype-pollution-security-rules": "^1.0.6",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-security-node": "^1.1.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-svelte": "^2.33.0",
    "gojs": "^2.3.10",
    "jsdom": "^22.1.0",
    "monaco-editor": "^0.41.0",
    "pdfjs-dist": "^2.16.105",
    "peggy": "^3.0.2",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.66.1",
    "sirv-cli": "^2.0.2",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-full-reload": "^1.0.5",
    "vite-plugin-iso-import": "^1.0.0",
    "vitest": "^0.34.3"
  },
  "type": "module"
}
